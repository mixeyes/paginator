{
  "name": "paginator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --quiet",
    "lint:quiet": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "format": "prettier ./**/*.{html,ts,tsx,json,xml,md,scss}",
    "format:quiet": "npm run format -- --list-different",
    "format:fix": "npm run format -- --write",
    "fix:all": "npm run format:fix && npm run lint:fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@rollup/plugin-strip": "^3.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-bundle-analyzer": "^0.10.6",
    "vite-plugin-compression2": "^1.3.0",
    "vitest": "^2.0.5"
  }
}
